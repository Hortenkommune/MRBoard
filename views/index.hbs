<table class="table">
  <thead>
    <tr>
      <th scope="col">Arrangør</th>
      <th scope="col">Tittel</th>
      <th scope="col">Start</th>
      <th scope="col">Slutt</th>
      <th scope="col">Møterom</th>
    </tr>
  </thead>
  <tbody>
  {{#if events}}
    {{#each events}}
    <tr>
      <td>{{this.organizer.emailAddress.name}}</td>
      <td>{{this.subject}}</td>
      <td>{{eventDateTime this.start.dateTime}}</td>
      <td>{{eventDateTime this.end.dateTime}}</td>
      <td>{{meetingroom this.location.displayName}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{else}}
  </tbody>
</table>
<div class="empty" style="display:none">
  <p>Ikke alle møter vises på skjermen.</p>
</div>
{{/if}}